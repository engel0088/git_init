<%# setup so that the following JS tracking code can be just copied/pasted %>
<%# (except from the capitalized 'State' that needs to be downcased) %>
<% zipcode = @zip_code%>
<% state = @state %>



<!--Search Ignite BEGIN action tracking script  version 4.0 -->
<script language="javascript" type="text/javascript">
<!--
